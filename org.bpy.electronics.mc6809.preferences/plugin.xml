            class="org.bpy.electronics.mc6809.preferences.core.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.bpy.electronics.mc6809.preferences.ui.MainPreferencePage"
            id="org.bpy.electronics.mc6809.preferences.mainpage"
