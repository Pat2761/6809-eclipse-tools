<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.bpy.electronics.mc6809.rcp.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.bpy.electronics.mc6809.rcp.Perspective"
            id="6809.perspective"
            name="MC6809">
      </perspective>
   </extension>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.bpy.electronics.mc6809.rcp.application"
            name="org.bpy.electronics.mc6809.rcp">
         <property
               name="appName"
               value="org.bpy.electronics.mc6809.rcp">
         </property>
         <property
               name="applicationXMI"
               value="org.bpy.electronics.mc6809.rcp/Application.e4xmi">
         </property>
      </product>
   </extension>
   <extension
         id="org.bpy.electronics.6809.project"
         name="6809 project"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.bpy.electronics.mc6809.rcp.project.ProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="org.bpy.electronics.mc6809.rcp.menuContribution1">
         <menu
               label="label">
         </menu>
      </menuContribution>
   </extension>
   <extension
         id="org.bpy.electronics.mc6809.rcp.fragment"
         point="org.eclipse.e4.workbench.model">
      <processor
            apply="always"
            beforefragment="true"
            class="org.bpy.electronics.mc6809.rcp.MainMenuModelProcessor">
      </processor>
      <fragment
            uri="fragment.e4xmi"
            apply="always">
      </fragment>
   </extension>

</plugin>
